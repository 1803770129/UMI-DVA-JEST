{
    "extends": "eslint-config-umi",
    "globals": {
        "$": true
    },
    "rules": {
        "no-debugger": 0,
        "indent": ["warn", 2, {
            "SwitchCase": 1
        }],
        "no-console": ["off"],
        "quotes": ["warn", "single"],
        "semi": ["error", "always"],
        "react/prop-types": 0,
        "eqeqeq": 0,
        "no-script-url": 0,
        "jsx-a11y/alt-text": 0,
        "jsx-a11y/anchor-is-valid": 0
    }
}
